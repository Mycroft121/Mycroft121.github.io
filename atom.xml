&lt;p&gt;&lt;div class=&quot;col-md-6 col-sm-3&quot;&gt;&lt;div class=&quot;content&quot;&gt;&lt;/div&gt;&lt;/div&gt;&lt;/p&gt;
&lt;h2 id=&quot;&quot;&gt;&lt;a href=&quot;#&quot; class=&quot;headerlink&quot; title=&quot;&quot;&gt;&lt;/a&gt;&lt;/h2&gt;&lt;p&gt;body{&lt;br&gt;    margin: 0;&lt;br&gt;}&lt;br&gt;.row{&lt;br&gt;    margin: 10px;&lt;br&gt;}&lt;br&gt;[class*=”col-“]{&lt;br&gt;    float: left;&lt;br&gt;}&lt;/p&gt;
&lt;p&gt;@media(min-width:768px){&lt;br&gt;    .col-md-1{&lt;br&gt;        width: 8.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-md-2{&lt;br&gt;        width: 16.66666667%;&lt;br&gt;    }&lt;br&gt;    .col-md-3{&lt;br&gt;        width: 25%;&lt;br&gt;    }&lt;br&gt;    .col-md-4{&lt;br&gt;        width: 33.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-md-5{&lt;br&gt;        width: 41.66666666%;&lt;br&gt;    }&lt;br&gt;    .col-md-6{&lt;br&gt;        width: 50%;&lt;br&gt;    }&lt;br&gt;    .col-md-7{&lt;br&gt;        width: 58.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-md-8{&lt;br&gt;        width: 66.66666667%;&lt;br&gt;    }&lt;br&gt;    .col-md-9{&lt;br&gt;        width: 75%;&lt;br&gt;    }&lt;br&gt;    .col-md-10{&lt;br&gt;        width: 83.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-md-11{&lt;br&gt;        width: 91.66666667%;&lt;br&gt;    }&lt;br&gt;    .col-md-12{&lt;br&gt;        width: 100%;&lt;br&gt;    }&lt;br&gt;}&lt;/p&gt;
&lt;p&gt;@media(max-width: 766px){&lt;br&gt;    .col-sm-1{&lt;br&gt;        width: 8.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-sm-2{&lt;br&gt;        width: 16.66666667%;&lt;br&gt;    }&lt;br&gt;    .col-sm-3{&lt;br&gt;        width: 25%;&lt;br&gt;    }&lt;br&gt;    .col-sm-4{&lt;br&gt;        width: 33.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-sm-5{&lt;br&gt;        width: 41.66666666%;&lt;br&gt;    }&lt;br&gt;    .col-sm-6{&lt;br&gt;        width: 50%;&lt;br&gt;    }&lt;br&gt;    .col-sm-7{&lt;br&gt;        width: 58.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-sm-8{&lt;br&gt;        width: 66.66666667%;&lt;br&gt;    }&lt;br&gt;    .col-sm-9{&lt;br&gt;        width: 75%;&lt;br&gt;    }&lt;br&gt;    .col-sm-10{&lt;br&gt;        width: 83.33333333%;&lt;br&gt;    }&lt;br&gt;    .col-sm-11{&lt;br&gt;        width: 91.66666667%;&lt;br&gt;    }&lt;br&gt;    .col-sm-12{&lt;br&gt;        width: 100%;&lt;br&gt;    }&lt;br&gt;}&lt;/p&gt;
